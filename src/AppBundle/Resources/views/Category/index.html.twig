{% extends '::base.html.twig' %}

{% block body -%}
<h1>Category list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Name</th>
            <th>Id</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        {% for entity in entities %}
        <tr>
            <td>{{ entity.name }}</td>
            <td>{{ entity.id }}</td>
        </tr>
        {% endfor %}
    </tbody>
</table>
{% endblock %}
